ve na doc\n\nconst API_BASE = 'https://api.themoviedb.org/3'//API QUE SERÁ USADA EM TODAS AS BASES DE LINK\n\n\n//pegando informaçoes //mandando um endpoint // requisitar e pegar o resultado e mandar de volta.\nconst basicFetch = async (endpoint) => {\n    const req = await fetch(`${API_BASE}${endpoint}`);\n    const json = await req.json();\n    return json;\n}\n\n\n\n//exportando a lista de exibição para o site\nexport default {\n    getHomeList: async () => { \n        return [\n            {\n                slug:'originals',\n                title: 'Originais da Netflix',\n                items: await basicFetch(`/discover/tv?with_network=213&language=pt-BR&api_key=${API_KEY}`)//filtrando para seriados somente da netflix\n            },\n            {\n                slug: 'trending',\n                title: 'Recomendados para você',\n                items: await basicFetch(`/trending/all/week?language=pt-BR&api_key=${API_KEY}`)//pegando filmes recomendados para destaque\n            },\n            {\n                slug: 'toprated',\n                title: 'Em alta',\n                items: await basicFetch(`/movie/top_rated?language=pt-BR&api_key=${API_KEY}`)\n            },\n            {\n                slug: 'action',\n                title: 'Ação',\n                items: await basicFetch(`/discover/movie?with_genres=28&language=pt-BR&api_key=${API_KEY}`)\n            },\n            {\n                slug: 'comedy',\n                title: 'Comédia',\n                 items: await basicFetch(`/discover/movie?with_genres=35&language=pt-BR&api_key=${API_KEY}`)\n            },\n            {\n                slug: 'horror',\n                title: 'Terror',\n                items: await basicFetch(`/discover/movie?with_genres=27&language=pt-BR&api_key=${API_KEY}`)\n            },\n            {\n                slug: 'romance',\n                title: 'Romance',\n                items: await basicFetch(`/discover/movie?with_genres=10749&language=pt-BR&api_key=${API_KEY}`)\n            },\n            {\n                slug: 'documentary',\n                title: 'Documentários',\n                 items: await basicFetch(`/discover/movie?with_genres=99&language=pt-BR&api_key=${API_KEY}`)\n            },\n        ];\n\n    },\n    getMovieInfo: async (movieId, type) => {//pegando informacao de um filme\n        let info = {};\n\n        if(movieId) {\n            switch(type){\n                case 'movie':\n                    info = await basicFetch(`/movie/${movieId}?language=pt-BR&api_key=${API_KEY}`);                \n                break;\n                case 'tv':\n                    info = await basicFetch(`/tv/${movieId}?language=pt-BR&api_key=${API_KEY}`);     \n                break;\n                default:\n                    info = null;\n                break;\n            }\n        }\n\n\n        return info;\n    }\n    \n}",["34","35"],"/home/fernando/Documents/Projetos/netflix-react/src/components/MovieRow.js",["36"],"/home/fernando/Documents/Projetos/netflix-react/src/components/FeaturedMovie.js",["37"],"/home/fernando/Documents/Projetos/netflix-react/src/components/Header.js",["38"],"import React from 'react';\n\nimport './Header.css';\nimport Netflix from \"../img/Netflix.png\";\n\n\nexport default ({black}) => {\n    return (\n        <header className={black ? 'black' : ''}>\n            <div className=\"header--logo\">\n                <a href=\"/\">\n                    <img src={Netflix} alt=\"logo-netflix\"/>\n                </a>\n            </div>     \n            <div className=\"header--user\">\n                 <a href=\"/\">\n                    <img src=\"https://pbs.twimg.com/profile_images/1240119990411550720/hBEe3tdn_400x400.png\" alt=\"usuario\"/>\n                </a>               \n            </div>         \n\n        </header>\n\n    );\n}",{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","severity":1,"message":"44","line":9,"co